{
  "name": "@thinkdeep/deep-template-consultancy",
  "version": "v0.0.8",
  "description": "Template to copy and paste for new custom elements using lit element.",
  "keywords": [
    "LitElement",
    "polymer"
  ],
  "author": "haydenmcp <hayden.mcparlane@gmail.com>",
  "homepage": "https://github.com/ThinkDeepTech/thinkdeep#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ThinkDeepTech/thinkdeep.git"
  },
  "scripts": {
    "dev": "web-dev-server --config web-dev-server.config.js",
    "dev:watch": "web-dev-server --config web-dev-server.config.js --watch",
    "tests": "web-test-runner \"test/**/*.test.js\" --node-resolve --coverage --playwright --browsers chromium firefox",
    "tests:watch": "web-test-runner \"test/**/*.test.js\" --node-resolve --watch --coverage --playwright --browsers chromium firefox",
    "copyindex": "cp index.html build",
    "copywc": "cp -r ../../node_modules/@webcomponents/webcomponentsjs/bundles build && cp ../../node_modules/@webcomponents/webcomponentsjs/webcomponents-loader.js build",
    "build": "rm -rf build && mkdir build && npm run copyindex && npm run copywc && rollup -c"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/parser": "^7.12.7",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/preset-env": "^7.12.11",
    "@rollup/plugin-eslint": "^8.0.1",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@thinkdeep/tools": "^0.0.8",
    "jest": "^27.0.6",
    "rollup": "^1.1.0",
    "rollup-plugin-babel": "^4.3.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-serve": "^1.1.0",
    "serve": "^10.1.1"
  },
  "dependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-modules-commonjs": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.14.7",
    "@babel/runtime": "^7.12.5",
    "@esm-bundle/chai": "^4.3.4",
    "@open-wc/karma-esm": "^4.0.0",
    "@open-wc/testing": "^2.5.33",
    "@open-wc/testing-helpers": "^1.8.12",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-replace": "^3.0.0",
    "@vaadin/router": "^1.7.4",
    "@web/dev-server-esbuild": "^0.2.12",
    "@web/dev-server-rollup": "^0.3.5",
    "@web/test-runner": "^0.13.15",
    "@web/test-runner-playwright": "^0.8.6",
    "@web/test-runner-puppeteer": "^0.10.0",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "chai": "^4.3.4",
    "es-dev-server": "^2.1.0",
    "karma": "^6.3.4",
    "karma-babel-preprocessor": "^8.0.1",
    "karma-browserify": "^8.0.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^2.0.3",
    "karma-edge-launcher": "^0.4.2",
    "karma-mocha": "^2.0.1",
    "karma-node-resolve-preprocessor": "^0.2.2",
    "karma-requirejs": "^1.1.0",
    "karma-rollup-plugin": "^0.2.4",
    "karma-rollup-preprocessor": "^7.0.7",
    "lit-element": "^2.5.1",
    "mocha": "^9.0.2",
    "requirejs": "^2.3.6",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-minify-html-literals": "^1.2.5",
